
  {% if show %}
<form method="post" action="{{ path('server_delete', {'id': server.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ server.id) }}">
    <button class="btn btn-dark float-right">Delete</button>
</form>
  {% else %}
        Unauthorized
  {% endif %}


